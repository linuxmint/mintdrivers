msgid "_Restart..."
msgstr "_Restartovat…"

#: usr/lib/linuxmint/mintdrivers/mintdrivers.py:211
msgid "Downloading drivers..."
msgstr "Stahování ovladačů…"

#: usr/lib/linuxmint/mintdrivers/mintdrivers.py:213
msgid "Installing drivers..."
msgstr "Instalace ovladačů…"

#: usr/lib/linuxmint/mintdrivers/mintdrivers.py:215
msgid "Removing drivers..."
msgstr "Odebírání ovladačů…"

